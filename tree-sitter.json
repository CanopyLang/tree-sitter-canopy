{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "canopy",
      "camelcase": "Canopy",
      "scope": "source.canopy",
      "path": ".",
      "file-types": [
        "can",
        "elm"
      ],
      "highlights": "queries/highlights.scm",
      "tags": "queries/tags.scm",
      "injection-regex": "queries/injections.scm"
    }
  ],
  "metadata": {
    "version": "5.8.0",
    "license": "MIT",
    "description": "Tree sitter definitions for canopy",
    "authors": [
      {
        "name": "Kolja Lampe",
        "email": "razzeee@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/CanopyLang/tree-sitter-canopy"
    }
  },
  "bindings": {
    "c": true,
    "node": true,
    "rust": true,
    "swift": true
  }
}